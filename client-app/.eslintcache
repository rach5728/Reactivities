t(url, body).then(sleep(1000)).then(responseBody),\r\n    del: (url: string) => axios.delete(url).then(sleep(1000)).then(responseBody)\r\n}\r\n\r\nconst Activities = {\r\n    list: (): Promise<IActivity[]> => requests.get('/activities'),\r\n    details: (id: string) => requests.get(`/activities/${id}`),\r\n    create: (activity: IActivity) => requests.post('/activities', activity),\r\n    update: (activity: IActivity) => requests.put(`/activities/${activity.id}`, activity),\r\n    delete: (id: string) => requests.del(`/activities/${id}`)\r\n}\r\n\r\nexport default {\r\n    Activities\r\n}","C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\layout\\LoadingComponent.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\layout\\App.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\home\\HomePage.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\layout\\ScrollToTop.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\activities\\dashboard\\ActivityListItem.tsx",[],["104","105"],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\stores\\activityStore.ts",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\activities\\details\\ActivityDetailedHeader.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\activities\\details\\ActivityDetailedInfo.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\activities\\details\\ActivityDetailedChat.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\features\\activities\\details\\ActivityDetailedSidebar.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\layout\\NotFound.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\client-app\\src\\app\\common\\form\\TextInput.tsx",[],"C:\\Users\\Rajneeshc\\Reactivites\\Reactivities\\cl